{
	"name": "ailurus",
	"contributors": [
		"Tristan Camejo <contact@tristancamejo.com>",
		"Nciklol <nickpleaseaddyouremail@example.com>"
	],
	"keywords": [
		"api",
		"discord",
		"bot",
		"node",
		"typescript"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/TeamEvie/ailurus.git"
	},
	"version": "0.0.2",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"imports": {
		"#structures/*": "./dist/structures/*.js",
		"#client/*": "./dist/client/*.js",
		"#utils/*": "./dist/utils/*.js"
	},
	"scripts": {
		"build": "tsc -b src && rollup -c scripts/rollup.bundle.ts && gen-esm-wrapper dist/index.js dist/index.mjs",
		"watch": "tsc-watch --onSuccess 'node dist/tests/index.js'",
		"test": "tsc -b src && node dist/tests/index.js",
		"lint": "eslint src --ext ts --fix"
	},
	"dependencies": {
		"dotenv": "^16.0.0",
		"undici": "^5.0.0",
		"ws": "^8.5.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-conventional": "^16.2.1",
		"@sapphire/eslint-config": "^4.3.4",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@types/eslint": "^8",
		"@types/node": "^17.0.25",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.20.0",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"gen-esm-wrapper": "^1.1.3",
		"lint-staged": "^12.3.8",
		"rollup": "^2.70.2",
		"rollup-plugin-version-injector": "^1.3.3",
		"standard-version": "^9.3.2",
		"tsc-watch": "^5.0.3",
		"typescript": "^4.6.3"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"packageManager": "yarn@3.2.0"
}
